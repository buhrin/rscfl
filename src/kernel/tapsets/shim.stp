%{
#include "rscfl/kernel/stap_shim.h"

#include "rscfl/kernel/probes.h"
%}

/**
 * INIT FUNCTIONS
 */
function probes_start:long()
%{
  STAP_RETVALUE = probes_init();
%}

function probes_end:long()
%{
  STAP_RETVALUE = probes_cleanup();
%}

function kpid:long()
%{
  STAP_RETVALUE = current->pid;
%}
